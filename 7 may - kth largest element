#include <iostream>
#include <algorithm>

using namespace std;
int main() {
    int n;
    cout << "enter size = ";
    cin >> n;
    int arr[n];
    for (int i = 0; i < n ; i++){
        cin >> arr[i];
    }
    int k;
    cout << "enter k value = ";
    cin >> k;
    sort(arr , arr + n);
    cout << arr[k];
    return 0;
}
